{
  "name": "jason-the-miner",
  "version": "0.5.0",
  "description": "A versatile Web scraper for Node.js",
  "author": "mawrkus <web@sparring-partner.be>",
  "license": "MIT",
  "homepage": "https://github.com/mawrkus/jason-the-miner",
  "bugs": {
    "url": "https://github.com/mawrkus/jason-the-miner/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mawrkus/jason-the-miner.git"
  },
  "keywords": [
    "web",
    "html",
    "scrape",
    "scraper",
    "scraping",
    "crawler",
    "harvester",
    "data extraction",
    "data grabbing",
    "data harvesting",
    "content extraction",
    "cheerio",
    "axios"
  ],
  "main": "es/JasonTheMiner.js",
  "bin": "bin/jason-the-miner.js",
  "engines": {
    "node": ">= 8.9"
  },
  "files": [
    "bin",
    "es",
    "lib"
  ],
  "scripts": {
    "lint": "eslint es/ bin/ demos/",
    "pretest": "npm run lint",
    "test": "jest es/",
    "tdd": "jest es/ --watch --verbose",
    "prerelease": "npm run test",
    "release": "npm version -m \"New version: %s\"",
    "postrelease": "npm run push && npm publish",
    "push": "git push origin master && git push origin --tags",
    "demos": "npm run demos:clean && node demos/index.js",
    "demos:clean": "clear && rm -rf demos/data/out/* && touch demos/data/out/.gitkeep",
    "demos:debug": "DEBUG=jason:* npm run demos"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "bluebird": "^3.5.0",
    "cheerio": "^0.22.0",
    "commander": "^2.9.0",
    "content-disposition": "^0.5.1",
    "csv-stringify": "^1.0.4",
    "debug": "^2.2.0",
    "lodash.flatten": "^4.4.0",
    "lodash.get": "^4.4.2",
    "lodash.mergewith": "^4.6.1",
    "mime": "^1.3.4",
    "mustache": "^2.2.1",
    "nodemailer": "^2.6.4",
    "ora": "^0.3.0",
    "pad-left": "^2.1.0",
    "uuid": "^2.0.3"
  },
  "devDependencies": {
    "eslint": "^4.18.2",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.12.3",
    "jest": "^22.4.2",
    "jest-when": "^1.1.0"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "<rootDir>/lib/",
      "<rootDir>/node_modules/"
    ],
    "testRegex": "/__tests__/.*\\.spec\\.js$"
  }
}
