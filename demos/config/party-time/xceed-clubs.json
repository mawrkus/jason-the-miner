{
  "load": {
    "http": {
      "baseURL": "https://xceed.me",
      "url": "/clubs-in-barcelona",
      "headers": {
        "Accept-Language": "en",
        "User-Agent": "Mozilla/5.0 (Macintosh; Intel Mac OS X 10.6; rv:42.0) Gecko/20100101 Firefox/42.0"
      }
    }
  },
  "parse": {
    "html": {
      "schemas": [
        {
          "clubs": {
            "_$": ".eventCard",
            "name": ".cardTitle",
            "address": "*[itemprop=streetAddress] < attr:content",
            "locality": "*[itemprop=addressLocality] < attr:content",
            "music": ".cardMusic |Â single-space",
            "atmosphere": ".cardAtmosphere | single-space"
          }
        },
        {
          "club-details": {
            "_$": "main",
            "name": "#info h1",
            "location": "#info .location | trim",
            "tags": "#tags > span",
            "description": "#venue-desc",
            "atmosphere": ".atmoOuter .tagsSecond | single-space",
            "door-policy": ".doorOuter .tagsSecond | single-space"
          }
        }
      ]
    }
  },
  "paginate": {
    "follow-link": {
      "selector": ".eventCardOuter > a",
      "mode": "all",
      "limit": "1",
      "rps": 10
    }
  },
  "transform": {
    "json-file": {
      "path": "demos/data/out/xceed-clubs.json"
    }
  }
}
