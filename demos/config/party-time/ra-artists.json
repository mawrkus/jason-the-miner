{
  "load": {
    "http": {
      "baseURL": "https://www.residentadvisor.net",
      "url": "/dj.aspx",
      "headers": {
        "Accept-Language": "en",
        "User-Agent": "Mozilla/5.0 (Macintosh; Intel Mac OS X 10.6; rv:42.0) Gecko/20100101 Firefox/42.0"
      }
    }
  },
  "parse": {
    "html": {
      "schemas": [
        {
          "artist": ".content-list span a | trim"
        }
      ]
    }
  },
  "transform": {
    "json-file": {
      "path": "demos/data/out/ra-artists.json"
    }
  }
}
