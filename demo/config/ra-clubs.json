{
  "load": {
    "http": {
      "baseURL": "https://www.residentadvisor.net",
      "url": "/clubs.aspx?ai=20",
      "headers": {
        "Accept-Language": "en",
        "User-Agent": "Mozilla/5.0 (Macintosh; Intel Mac OS X 10.6; rv:42.0) Gecko/20100101 Firefox/42.0"
      }
    }
  },
  "parse": {
    "html": {
      "schemas": [
        {
          "main ul.content-list li section.content.clearfix div.plus8 div.fl.col4-6 ul ul li.clearfix": {
            "club": "div.fl a | trim",
            "location": "div.fl.grey.mobile-off |Â trim"
          }
        },
        {
          "body": {
            "club-name": "#sectionHead span[itemprop=name] | trim",
            "maps-link": "main #detail ul.clearfix li:last-child a << attr:href"
          }
        }
      ]
    }
  },
  "paginate": {
    "next-link": {
      "selector": "main ul.content-list li section.content.clearfix div.plus8 div.fl.col4-6 ul ul li.clearfix div.fl a",
      "limit": 1
    }
  },
  "transform": {
    "json-file": {
      "path": "demo/data/out/ra-clubs.json"
    }
  }
}
