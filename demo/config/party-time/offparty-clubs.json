{
  "load": {
    "file": {
      "path": "demo/data/in/offparty-clubs.html"
    },
    "http": {
      "baseURL": "http://offparty.com",
      "url": "/clubs.php",
      "headers": {
        "Accept-Language": "en",
        "User-Agent": "Mozilla/5.0 (Macintosh; Intel Mac OS X 10.6; rv:42.0) Gecko/20100101 Firefox/42.0"
      }
    }
  },
  "parse": {
    "html": {
      "schemas": [
        {
          "clubs": {
            "_$": ".singleton",
            "name": "h2",
            "address": ".shp < html",
            "city": ".shp > a"
          }
        }
      ]
    }
  },
  "transform": {
    "json-file": {
      "path": "demo/data/out/offparty-clubs.json"
    }
  }
}
