{
  "load": {
    "http": {
      "url": "https://www.residentadvisor.net/events.aspx",
      "headers": {
        "Accept-Language": "en",
        "User-Agent": "Mozilla/5.0 (Macintosh; Intel Mac OS X 10.6; rv:42.0) Gecko/20100101 Firefox/42.0"
      }
    }
  },
  "parse": {
    "html": {
      "schemas": [
        {
          ".event-item": {
            "name": "h1[itemprop=summary] > a",
            "artists": ".bbox .grey |Â trim",
            "venue": "h1[itemprop=summary] > span > a",
            "date": "time",
            "attendees": ".attending > span"
          }
        }
      ]
    }
  },
  "-paginate": {
    "follow-link": {
      "selector": "a#load_next_episodes",
      "limit": 1
    }
  },
  "transform": {
    "json-file": {
      "path": "demo/data/out/ra-events.json"
    }
  }
}
