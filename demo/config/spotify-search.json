{
  "load": {
    "http": {
      "url": "https://api.spotify.com/v1/search",
      "params": {
        "type": "artist",
        "q": "flying",
        "offset": 0,
        "limit": 25
      }
    }
  },
  "paginate": {
    "url-param": {
      "param": "offset",
      "inc": 25,
      "limit": 4,
      "mode": "parallel"
    }
  },
  "parse": {
    "json": {
      "schemas": [
        {
          "artists": {
            "_$": "artists.items",
            "name": "name",
            "popularity": "popularity",
            "image": "x-images[1].url"
          }
        }
      ]
    }
  }
}
