{
  "load": {
    "http": {
      "url": "https://www.npmjs.com/browse/star"
    }
  },
  "paginate": {
    "url-param": {
      "param": "offset",
      "inc": 36,
      "limit": 4,
      "mode": "parallel"
    }
  },
  "parse": {
    "html": {
      "schemas": [
        {
          "packages": {
            "_$": ".package-details",
            "name": ".name",
            "version": ".version",
            "description": ".description",
            "author": ".author > a:last-child"
          }
        }
      ]
    }
  },
  "transform": {
    "tpl": {
      "templatePath": "demo/data/in/npm-starred.html.tpl",
      "outputPath": "demo/data/out/npm-starred.html"
    }
  }
}
